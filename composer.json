{
    "name": "fotofix/real-estate-photo-enhancement",
    "description": "AI-powered real estate photo enhancement service",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "FotoFix Team",
            "email": "contact@fotofix.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "stripe/stripe-php": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FotoFix\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"if (!file_exists('api/vendor')) { mkdir('api/vendor', 0755, true); }\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
